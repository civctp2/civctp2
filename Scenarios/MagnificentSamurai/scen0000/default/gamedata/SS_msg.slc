alertbox 'SSAmbushGonza' {
	Show(); Title(ID_SS_BLANK);
	if (TerrainType(sevenSamurai[3].location) == 0) {
		SetString(0, ID_SS_AMBUSH_FOREST);
	} elseif (TerrainType(sevenSamurai[3].location) == 4) {
		SetString(0, ID_SS_AMBUSH_GRASSLAND);
	} elseif (TerrainType(sevenSamurai[3].location) == 6) {
		SetString(0, ID_SS_AMBUSH_SWAMP);	
	} elseif (TerrainType(sevenSamurai[3].location) == 7) {
		SetString(0, ID_SS_AMBUSH_JUNGLE);
	} elseif (TerrainType(sevenSamurai[3].location) == 5) {
		SetString(0, ID_SS_AMBUSH_DESERT);
	} elseif (TerrainType(sevenSamurai[3].location) == 18) {
		SetString(0, ID_SS_AMBUSH_DUNES);
	} elseif (TerrainType(sevenSamurai[3].location) == 13) {
		SetString(0, ID_SS_AMBUSH_BEACH);
	} elseif (TerrainType(sevenSamurai[3].location) == 8 || TerrainType(sevenSamurai[3].location) == 9) {
		SetString(0, ID_SS_AMBUSH_MOUNTAIN);
	} else {
		SetString(0, ID_SS_AMBUSH_PLAINS);
	}
	Text(ID_SS_AMBUSH_GONZA);
	Button(ID_SS_BUTTON_OK) {
		Kill();
	}
}

